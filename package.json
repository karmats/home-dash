{
  "name": "home-dash",
  "version": "0.1.0",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "form-data": "^4.0.0",
    "googleapis": "^110.0.0",
    "node-fetch": "^2.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-svg": "^15.1.21",
    "rss-parser": "^3.12.0",
    "url-loader": "^4.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.1",
    "camelcase": "^7.0.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^8.1.5",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.28.1"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm:client\" \"npm:server\"",
    "client": "vite",
    "preserver": "npm run build:server",
    "server": "nodemon build/server/server.js",
    "prebuild:server": "rm -rf build/server",
    "build:server": "tsc -p tsconfig.server.json",
    "prebuild:client": "rm -rf build/client",
    "build:client": "vite build",
    "postbuild:client": "cp -r public/svgs build/client",
    "build": "npm run build:client && npm run build:server",
    "test": "vitest",
    "lint": "eslint ./",
    "format": "prettier ./**/*.{html,json,md,css,ts,tsx} --write"
  }
}
