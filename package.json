{
  "name": "home-dash",
  "version": "0.1.0",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "form-data": "^4.0.0",
    "googleapis": "^118.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-svg": "^16.1.17",
    "rss-parser": "^3.13.0",
    "url-loader": "^4.1.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.1",
    "camelcase": "^7.0.1",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.20.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm:client\" \"npm:server\"",
    "client": "vite",
    "preserver": "npm run build:server",
    "server": "nodemon build/server/server.js",
    "prebuild:server": "rm -rf build/server",
    "build:server": "tsc -p tsconfig.server.json",
    "prebuild:client": "rm -rf build/client",
    "build:client": "vite build",
    "postbuild:client": "cp -r public/svgs build/client",
    "build": "npm run build:client && npm run build:server",
    "test": "vitest",
    "lint": "eslint ./",
    "format": "prettier ./**/*.{html,json,md,css,ts,tsx} --write"
  }
}
