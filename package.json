{
  "name": "home-dash",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/karmats/home-dash",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@svgr/webpack": "^5.3.1",
    "@testing-library/react": "^9.4.1",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.7",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^25.2.6",
    "jest-watch-typeahead": "^0.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "webpack": "^4.42.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "dependencies": {
    "css-loader": "^3.5.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "form-data": "^3.0.0",
    "googleapis": "^48.0.0",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-svg": "^11.0.16",
    "resolve": "^1.15.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-pnp": "^1.2.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail 'node scripts/start.js' 'npm run server'",
    "server": "concurrently --kill-others-on-fail 'npm run build:server -- -w' 'nodemon build/server/server.js'",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "node scripts/build.js",
    "build": "npm run build:client && npm run build:server",
    "test": "node scripts/test.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.{js,jsx,ts,tsx}",
      "!client/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testRegex": "((\\.|/)test)\\.tsx?$",
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
